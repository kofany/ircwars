syntax "conf" "\.conf$"

# Komentarze na cyan

# Pierwsze litery w linii na czerwono (wykluczając #)
color brightred "^[^#]"

# Znak procentu na różowo
color magenta "%"
# Adresy IPv4 i prefixy na niebiesko

# Adresy IPv4 i prefixy na niebiesko
color brightblue "\b((25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.){3}(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)(/[0-9]{1,2})?\b"

# Adresy IPv6 na niebiesko
color brightblue "([0-9a-fA-F]{1,4}:){7}[0-9a-fA-F]{1,4}(/([0-9]{1,3}|[0-9a-fA-F]{1,4}))?"
color brightblue "([0-9a-fA-F]{1,4}:){7}[0-9a-fA-F]{1,4}(/([0-9]{1,3}|[0-9a-fA-F]{1,4}))?"
color brightblue "([0-9a-fA-F]{1,4}:){1,7}:"
color brightblue "::([0-9a-fA-F]{1,4}:){0,6}[0-9a-fA-F]{1,4}"
color brightblue "([0-9a-fA-F]{1,4}:){1,6}:[0-9a-fA-F]{1,4}"
color brightblue "/[0-9]{1,3}"
color brightblue "[a-zA-Z0-9-]+\.[a-zA-Z]{2,}"
color brightblue "\*\@"
color brightgreen "#.*"
